{
  "name": "ark4ne/phalcon-luxury",
  "description": "Phalcon extended framework. (Luxury:App)",
  "keywords": [
    "framework",
    "phalcon",
    "luxury"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Ark4ne (Guillaume Allegret)",
      "email": "gui.allegret@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Ark4ne/phalcon-luxury-framework"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-phalcon": ">=3.0",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ark4ne/phalcon-luxury-framework": "*"
  },
  "require-dev": {
    "phalcon/devtools": "dev-master",
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  }
}